{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p watch:routify watch:rollup watch:nim",
    "__build": "rollup -c",
    "build": "routify -b && rollup -c",
    "prebuild": "routify --single-build",
    "watch:rollup": "rollup -c -w",
    "watch:routify": "routify",
    "watch:nim": "nim project watch .. --exclude web",
    "start": "nim project:serve-web",
    "validate": "svelte-check"
  }, 
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@roxi/routify": "^2.18.0",
    "@tsconfig/svelte": "^1.0.0",
    "nimbella-cli": "git+https://github.com/nimbella/nimbella-cli.git",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.38.2",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@sveltech/routify": "^1.1.1-1",
    "bootstrap": "^5.0.1",
    "sveltestrap": "^5.0.6"
  }
}
